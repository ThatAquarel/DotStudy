{
	"name": "dotstudy",
	"displayName": "DotStudy Notebook",
	"description": "",
	"author": {
		"name": "aquarel"
	},
	"publisher": "aquarel",
	"repository": {
		"url": "https://github.com/ThatAquarel/DotStudy"
	},
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:dotstudy-language",
		"onCustomEditor:dotstudy.preview",
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"grammars": [
			{
				"scopeName": "markdown.study.inline",
				"path": "./syntaxes/md-study-inline.tmLanguage.json",
				"injectTo": [
					"text.html.markdown"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"discord.js": "^12.x",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.2.0",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2",
		"discord.js": "^12.x",
		"reflect-metadata": "^0.1.13"
	}
}
