{
	"name": "dotstudy",
	"displayName": "DotStudy Notebook",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onNotebook:dotstudy-notebook"
	],
	"main": "./out/extension.js",
	"contributes": {
		"notebooks": [
            {
                "id": "dotstudy-notebook",
                "type": "dotstudy-notebook",
                "displayName": "DotStudy Notebook",
                "selector": [
                    {
                        "filenamePattern": "*.study"
                    }
                ]
            }
        ],
		"languages": [{
            "id": "dotstudy-language",
            "aliases": ["DotStudy"],
            "extensions": [".study"],
            "configuration": "./syntaxes/language-configuration.json"
        }],
        "grammars": [{
            "language": "dotstudy-language",
            "scopeName": "source.study",
            "path": "./syntaxes/study.tmLanguage.json"
        }]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	}
}
